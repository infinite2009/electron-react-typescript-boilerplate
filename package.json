{
  "name": "nezha-client",
  "version": "0.0.0",
  "description": "哪吒桌面端",
  "author": "luodongyang@theduapp.com",
  "license": "MIT",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "hot"
  ],
  "main": "main-process.js",
  "scripts": {
    "build": "npx webpack --env.NODE_ENV=development --env.target=render && npx webpack --env.NODE_ENV=development --env.target=main",
    "build-prod": "npx webpack --env.NODE_ENV=production --env.target=render && npx webpack --env.NODE_ENV=production --env.target=main",
    "server": "npx webpack-dev-server --env.NODE_ENV=development --env.target=render --hot",
    "start": "npx webpack --env.NODE_ENV=development --env.target=main && npx electron ./dist/.",
    "pack": "npx rimraf ./pack && npm run build-prod && npx electron-packager ./dist --out ./pack --overwrite",
    "pack:dmg": "npm run pack && electron-installer-dmg ./pack/nezha-client-darwin-x64/nezha-client.app nezha-client --out pack"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.8.2",
    "electron-log": "^4.3.0",
    "fontsource-roboto": "^3.0.3",
    "jsonfile": "^6.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/antd": "^1.0.0",
    "@types/jsonfile": "^6.0.0",
    "@types/node": "^14.14.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.15.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "electron": "^10.1.4",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.5.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "postinstall": "electron-builder install-app-deps"
}
